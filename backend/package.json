{
  "name": "adcash-campaign-management",
  "version": "1.0.0",
  "description": "campaign-management web app for Adcash",
  "main": "dist/src/server.js",
  "scripts": {
    "test": "docker-compose -f docker-compose.only-db.yml up -d && npm run db:push && jest -i --colors  --silent=false --verbose --detectOpenHandles && docker-compose -f docker-compose.only-db.yml down",
    "dev": "cross-env NODE_ENV=dev nodemon src/server.ts",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "build": "rimraf build && tsc -p tsconfig.json",
    "start": "node dist/src/server.js",
    "db:seed": "ts-node prisma/seed.ts"
  },
